<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.local.keyboard_agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>INSTALL_DIR_PLACEHOLDER/keyboard_agent</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/keyboard_agent.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/keyboard_agent.log</string>
</dict>
</plist>